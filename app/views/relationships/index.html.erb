<h1>Relationships#index</h1>

<div>
  <% @users.each do |user| %>
  <p>
    <%= user.first_name  %>
    <%= user.last_name %>
    <%= user.id %>
    Current_user:<%= @current_user.id %>

    <%= button_to "Like", relationships_path(:user_id => user.id),  :class => "btn btn-default" %>


  </p>
  <% end %>

  <% @user_likees.each do |user| %>
    <p>
      Current user likes <%= user.liker_id %>
    </p>

  <% end %>

  <% @likers.each do |user| %>
  <p>
    Who like current User<%= user.likee_id  %>
  </p>
  <% end %>
<!--
  <%# if user.liker_id == user.likee_id %>
    <p>You Matched! </p>

  <%# end %> -->
</div>
