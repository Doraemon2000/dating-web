<h1 class="my-matches" >&lt;h1> &#10084; My Matches &#10084; &lt;/h1></h1>

<div class="matched-container">
  <% @users.each do |user| %>



      <% if user.image.present? %>

      <div class="matched-index-wrapper">

        <div class="matched-index-nested">

            <%= link_to image_tag(user.photos.last, :class => "index-image", :alt => user.first_name), user_path(user) %>

          <% else %>

            <%= image_tag 'img.jpg', :class => "index-image"  %>

          <% end %>
        </div>


        <div class="matched-index-nested">

          <%= link_to "Checkout #{user.first_name}", user_path(user) %>


          <%= link_to 'Chat ?', controller: "mailboxes", action: "new", id: user.id %>

          <%= button_to "Remove", unlike_path(user.id), method: :delete, :class => "btn btn-default", :id => "like_button" %>




        </div>

      </div>


  <% end %>

</div>
